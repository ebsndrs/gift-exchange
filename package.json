{
  "homepage": "https://ebsndrs.github.io/gift-exchange",
  "name": "gift-exchange",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "typescript": "^3.8.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "tailwindcss": "^1.2.0",
    "@tailwindcss/ui": "^0.1.3",
    "postcss-cli": "^7.1.0",
    "autoprefixer": "^9.7.6",
    "gh-pages": "^2.2.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/css/tailwind.css -o src/tailwind.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/tailwind.css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
